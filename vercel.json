{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }, 
        {
            "src": "/auth/register",
            "method": "POST",
            "dest": "/controllers/authController.js"
        },
        {
            "src": "/auth/register",
            "method": "POST",
            "dest": "/models/user.js"
        }
    ]
}